
<%= render 'form' %>

<div class="modal fade" id = 'emailModal'>
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        
        <h4 class="modal-title">Predicted Email</h4>
      </div>
      <div class="modal-body">
        <%= image_tag "mail.png", :id => "success" %>
        <%= image_tag "fail_whale.gif", :id => "fail" %>
        <p> </p>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default main-button" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>